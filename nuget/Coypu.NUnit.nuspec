<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
    <id>Coypu.NUnit</id>
    <version>3.0.0</version>
    <title>Coypu.NUnit</title>
	  <authors>Adrian Longley, Jakub Obstarczyk</authors>
	  <owners>Adrian Longley</owners>
	  <license type="expression">MIT</license>
	  <projectUrl>https://github.com/featurist/coypu</projectUrl>
	  <description>
	    NUnit matchers for Coypu.
      This package includes the Coypu.NUnit 3 framework assembly, which is referenced by your tests.

      Supported platforms:
      - .NET Framework 4.5+
      - .NET Standard 2.0+
      - .NET Core 2.0+
    </description>
	  <releaseNotes>
	    This package includes breaking changes as:
	    - support for net45+, netstandard2.0+ and netcore2.0 frameworks
	  </releaseNotes>
	  <copyright>Copyright (c) 2018 Adrian Longley and Contributors</copyright>
	  <repository type="git" url="https://github.com/featurist/coypu" />
		<dependencies>
			<group targetFramework=".NETFramework4.5">
				<dependency id="Coypu" version="[3.0.0,4)" />
				<dependency id="NUnit" version="[3.11.0,4)" />
			</group>
			<group targetFramework=".NETStandard2.0">
				<dependency id="Coypu" version="[3.0.0,4)" />
				<dependency id="NUnit" version="[3.11.0,4)" />
				<dependency id="NETStandard.Library" version="[2.0.3,3)" />
			</group>
		</dependencies>
	</metadata>
	<files>
		<file src="..\LICENSE.txt" />
		<file src="..\README.md" />
		<file src="..\History.md" />
		<file src="..\src\Coypu.NUnit\bin\Debug\net45\Coypu.NUnit.dll" target="lib\net45" />
		<file src="..\src\Coypu.NUnit\bin\Debug\net45\Coypu.NUnit.pdb" target="lib\net45" />
		<file src="..\src\Coypu.NUnit\bin\Debug\net45\Coypu.NUnit.xml" target="lib\net45" />
		<file src="..\src\Coypu.NUnit\bin\Debug\netstandard2.0\Coypu.NUnit.dll" target="lib\netstandard2.0" />
		<file src="..\src\Coypu.NUnit\bin\Debug\netstandard2.0\Coypu.NUnit.pdb" target="lib\netstandard2.0" />
		<file src="..\src\Coypu.NUnit\bin\Debug\netstandard2.0\Coypu.NUnit.xml" target="lib\netstandard2.0" />
	</files>
</package>